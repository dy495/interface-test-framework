<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.haisheng.framework.dao.IPvUvDao">
    <resultMap id="ApplicationMap" type="com.haisheng.framework.model.bean.PVUV">
        <result property="id" column="id"/>
        <result property="mapId" column="map_id"/>
        <result property="regionId" column="region_id" />
        <result property="entranceId" column="entrance_id" />
        <result property="pv" column="pv" />
        <result property="status" column="status" />
        <result property="updateTime" column="update_time" />
    </resultMap>

    <!-- 所有字段 -->
    <sql id="allFields">
        id,map_id,region_id,entrance_id,pv,status,update_time
    </sql>


    <!-- insert application-->
    <insert id="insert"
            parameterType="com.haisheng.framework.model.bean.PVUV"
            useGeneratedKeys="true"
            keyProperty="id">
        <![CDATA[
            insert into t_pv_uv (
                map_id,
                region_id,
                entrance_id,
                pv,
                status,
                update_time
            ) values (
                #{mapId},
                #{regionId},
                #{entranceId},
                #{pv},
                #{status},
                #{updateTime}
            ) ON DUPLICATE KEY UPDATE
            pv=#{pv},
            status=#{status},
            update_time=#{updateTime}
        ]]>
    </insert>



</mapper>